      "keyColor": "blue"
    },
    {
      "type": "swap",
      "key": "├╼ Swap",
      "keyColor": "blue"
    },
    {
      "type": "processes",
      "key": "└╼ Processes",
      "keyColor": "blue"
    },
    {
