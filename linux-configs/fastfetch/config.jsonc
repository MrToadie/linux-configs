{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
      //"type": "auto",
      "source": "arch",
      "width": 65,
      "height": 35,
      "padding": {
        "top": 3
      },
      "color": {
		    "1": "blue",
		    "2": "blue"
		}
  },
  "display": {
    "separator": ": ",
    "color": "blue"
  },
  "modules": [
    "break",
    {
      "type": "title",
    },
    {
      "type": "custom",
      "format": "╾─────────────────────────────────────────────────────────────╼",
      "outputColor": "blue"
    },
    {
      "type": "os",
      "key": "┌╼ OS",
      "keyColor": "blue"
    },
    {
      "type": "command",
      "key": "├╼ OS Age",
      "keyColor": "blue",
      "text": "birth_install=$(stat -c %W /); current=$(date +%s); time_progression=$((current - birth_install)); days_difference=$((time_progression / 86400)); echo $days_difference days"
    },
    {
      "type": "kernel",
      "key": "├╼ Kernel",
      "keyColor": "blue"
    },
    {
      "type": "uptime",
      "key": "├╼ Uptime",
      "keyColor": "blue"
    },
    {
      "type": "packages",
      "key": "├╼ Packages",
      "keyColor": "blue"
    },
    {
      "type": "shell",
      "key": "└╼ Shell",
      "keyColor": "blue"
    },
    {
      "type": "custom",      
      "format": "╾─────────────────────────────────────────────────────────────╼"
    },
    {
      "type": "cpu",
      "key": "┌╼ CPU",
      "showPeCoreCount": true,
      "temp": true,
      "keyColor": "blue"
    },
    {
      "type": "gpu",
